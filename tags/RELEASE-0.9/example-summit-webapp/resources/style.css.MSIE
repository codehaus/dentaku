body, p { font-family: Verdana, sans-serif; font-size: 8pt; padding: 0px; margin: 0.25em 0; line-height: 1.00; color: #006699; }
table, form, tr, td { padding: 0px; margin: 0.0em 0; color: #006699; font-size: 8pt; border: 0px; } 
h1 { font-family: Verdana, sans-serif; font-size: 16pt; padding: 0px; margin: 0.25em 0; line-height: 1.00; color: #006699; } 
h2 { font-family: Verdana, sans-serif; font-size: 11pt; padding: 0px; margin: 0.25em 0; line-height: 1.00; color: #006699; } 
h3 { font-family: Verdana, sans-serif; font-size: 10pt; padding: 0px; margin: 0.25em 0; line-height: 1.00; color: #006699; } 
h4 { font-family: Verdana, sans-serif; font-size: 9pt; padding: 0px; margin: 0.25em 0; line-height: 1.00; color: #006699; } 
